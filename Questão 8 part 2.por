programa
{
	
	funcao inicio()
	{
		inteiro N // N = número

		escreva("\nDigite um número: ")
		leia(N)
		limpa()

		se (N >100) {

			escreva(N)
		}

		senao se (N<=100) {

			N = 0

			escreva(N)
		}
	}
}
